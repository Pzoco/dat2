Team NinjaMonkeys1

Regiment Soldier
{
	Size = 20;
	Type = Melee;
	Range = 10;
	Damage = 2;
	Health = 10;
	Movement = 30;
	AttackSpeed = 1;
	RegimentPosition = Position(0,0);
	Behaviour SilentMonkeysBehavoiur1
	{
		Regiment enemy = SearchForEnemies();
		if (enemy.Distance <= Range)
		{
			Attack(enemy);
		}
		else if (enemy.Distance <= Range+Movement)
		{
			while(enemy.Distance < Range)
			{
				MoveTowards(enemy);
			}
			Attack(enemy);
		}
		MoveTowards(enemy);
	}
}
