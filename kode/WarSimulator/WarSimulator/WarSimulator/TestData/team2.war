Team Blue

Regiment BlueWarrior1
{
	Size = 10;
	Type = Melee;
	Damage = 4;
	Health = 4;
	Movement = 1;
	AttackSpeed = 1;
	RegimentPosition = Position(8,4);
	Behaviour SilentMonkeysBehavoiur1
	{
		Regiment enemy = SearchForEnemies();
		if (enemy.Distance <= Range)
		{
			Attack(enemy);
		}
		else
		{
			MoveTowards(enemy);
			Attack(enemy);
		}
	}
}
